/* ============================================================================
   Package Exporter - Component-Specific Styles
   Shared design system styles are in YucpDesignSystem.uss
   ============================================================================ */

/* ============================================================================
   MODERN DESIGN SYSTEM - shadcn/ui inspired
   ============================================================================ */

/* Surface and Popover - Soft surfaces with subtle borders and shadows */
.yucp-surface {
    background-color: rgba(30, 30, 30, 1);
}

.yucp-popover {
    min-width: 280px;
    max-width: 340px;
    background-color: rgba(35, 35, 35, 1);
    border-width: 1px;
    border-color: rgba(255, 255, 255, 0.08);
    border-radius: 8px;
    padding: 12px;
    overflow-x: hidden;
    overflow-y: auto;
}

.yucp-popover-content {
    overflow-x: hidden;
    min-width: 0;
}

/* Toolbar - Header row */
.yucp-toolbar {
    flex-direction: row;
    align-items: center;
    justify-content: space-between;
    padding: 8px 12px;
    background-color: rgba(30, 30, 30, 1);
}

.yucp-toolbar-left {
    flex-direction: row;
    align-items: center;
    flex-shrink: 0;
}

.yucp-toolbar-left > * {
    margin-right: 8px;
}

.yucp-toolbar-right {
    flex-direction: row;
    align-items: center;
    flex-grow: 1;
    justify-content: flex-end;
    min-width: 0;
}

.yucp-toolbar-right > * {
    margin-left: 8px;
}

/* ============================================================================
   GENERAL BUTTONS - Standardized
   ============================================================================ */

/* Base Button Style */
Button {
    height: 30px;
    border-radius: 6px;
    font-size: 12px;
    background-color: rgba(255, 255, 255, 0.05);
    border-width: 1px;
    border-color: rgba(255, 255, 255, 0.1);
    color: #e0e0e0;
    margin-left: 2px;
    margin-right: 2px;
    padding-left: 12px;
    padding-right: 12px;
    transition-duration: 0.15s;
    transition-property: background-color, border-color, scale;
}

Button:hover {
    background-color: rgba(255, 255, 255, 0.1);
    border-color: rgba(255, 255, 255, 0.2);
}

Button:active {
    background-color: rgba(255, 255, 255, 0.02);
}

/* Primary Action Button (Teal) */
.yucp-button-primary {
    background-color: rgba(54, 191, 177, 0.2);
    border-color: rgba(54, 191, 177, 0.5);
    color: #36BFB1;
}

.yucp-button-primary:hover {
    background-color: rgba(54, 191, 177, 0.3);
    border-color: rgba(54, 191, 177, 0.7);
}

/* Text-only / Ghost Button */
.yucp-button-text {
    background-color: transparent;
    border-width: 0;
    color: rgba(255, 255, 255, 0.5);
}

.yucp-button-text:hover {
    background-color: rgba(255, 255, 255, 0.05);
    color: rgba(255, 255, 255, 0.8);
}

/* Outline Button - Compact with icon */
.yucp-btn-outline {
    background-color: transparent;
    border-width: 1px;
    border-color: rgba(255, 255, 255, 0.12);
    color: rgba(255, 255, 255, 0.85);
    padding: 6px 10px;
    border-radius: 6px;
    font-size: 12px;
    height: 30px;
    transition-duration: 0.15s;
    transition-property: background-color, border-color;
    flex-direction: row;
    align-items: center;
}

.yucp-btn-outline:hover {
    background-color: rgba(255, 255, 255, 0.04);
    border-color: rgba(255, 255, 255, 0.18);
}

.yucp-btn-outline:active {
    background-color: rgba(255, 255, 255, 0.06);
}

.yucp-btn-outline.active {
    background-color: rgba(54, 191, 177, 0.15);
    border-color: rgba(54, 191, 177, 0.4);
    color: rgba(54, 191, 177, 1);
}

.yucp-btn-outline > * {
    margin-right: 6px;
}

.yucp-btn-outline > *:last-child {
    margin-right: 0;
}

.yucp-btn-icon {
    width: 14px;
    height: 14px;
    flex-shrink: 0;
}

.yucp-badge-count {
    margin-left: 6px;
    padding: 2px 6px;
    background-color: rgba(54, 191, 177, 0.25);
    border-radius: 10px;
    font-size: 10px;
    color: rgba(54, 191, 177, 1);
    min-width: 18px;
    -unity-text-align: middle-center;
    height: 16px;
    flex-shrink: 0;
}

/* Input - Rounded search field */
.yucp-input {
    background-color: rgba(40, 40, 40, 1);
    border-width: 1px;
    border-color: rgba(255, 255, 255, 0.1);
    border-radius: 8px;
    padding: 6px 12px;
    font-size: 12px;
    color: rgba(255, 255, 255, 0.9);
    min-height: 30px;
    transition-duration: 0.15s;
    transition-property: border-color, background-color;
}

.yucp-input:hover {
    border-color: rgba(255, 255, 255, 0.15);
}

.yucp-input:focus {
    border-color: rgba(54, 191, 177, 0.5);
    background-color: rgba(42, 42, 42, 1);
}

.yucp-search-container {
    flex-direction: row;
    align-items: center;
    flex-grow: 1;
    max-width: 320px;
    min-width: 200px;
}

.yucp-search-container > * {
    margin-right: 8px;
}

.yucp-search-clear {
    width: 20px;
    height: 20px;
    padding: 0;
    background-color: transparent;
    border-width: 0;
    color: rgba(255, 255, 255, 0.5);
    font-size: 14px;
    -unity-text-align: middle-center;
    border-radius: 4px;
    flex-shrink: 0;
}

.yucp-search-clear:hover {
    background-color: rgba(255, 255, 255, 0.08);
    color: rgba(255, 255, 255, 0.9);
}

/* ============================================================================
   ACTIVE FILTER CHIPS ROW
   ============================================================================ */
.yucp-active-filters-row {
    flex-direction: row;
    align-items: center;
    flex-wrap: wrap;
    padding: 8px 12px;
    border-bottom-width: 1px;
    border-bottom-color: rgba(255, 255, 255, 0.1);
    background-color: rgba(54, 191, 177, 0.05);
    min-height: 32px;
}

.yucp-active-filters-row > * {
    margin-right: 4px;
    margin-bottom: 4px;
}

/* Popover Sections */
.yucp-popover-section {
    margin-bottom: 12px;
}

.yucp-popover-section-title {
    font-size: 10px;
    -unity-font-style: bold;
    color: rgba(255, 255, 255, 0.5);
    letter-spacing: 0.5px;
    margin-bottom: 6px;
    padding-bottom: 6px;
    border-bottom-width: 1px;
    border-bottom-color: rgba(255, 255, 255, 0.08);
}

.yucp-popover-actions {
    flex-direction: row;
    justify-content: flex-end;
    margin-top: 12px;
    padding-top: 12px;
    border-top-width: 1px;
    border-top-color: rgba(255, 255, 255, 0.08);
}

.yucp-popover-actions > * {
    margin-left: 8px;
}

/* Row Item - Full-width clickable checkbox row */
.yucp-row-item {
    flex-direction: row;
    align-items: center;
    padding: 8px 10px;
    margin-bottom: 2px;
    border-radius: 6px;
    transition-duration: 0.15s;
    transition-property: background-color;
    min-height: 30px;
    width: 100%;
}

.yucp-row-item:hover {
    background-color: rgba(255, 255, 255, 0.04);
}

.yucp-row-item:active {
    background-color: rgba(255, 255, 255, 0.06);
}

.yucp-row-item.selected {
    background-color: rgba(54, 191, 177, 0.12);
}

.yucp-row-item .yucp-toggle {
    margin-right: 10px;
    margin-bottom: 0;
    flex-shrink: 0;
}

.yucp-row-item-label {
    flex-grow: 1;
    font-size: 12px;
    color: rgba(255, 255, 255, 0.9);
}

/* Sort Option Row - List-style selection */
.yucp-sort-option {
    flex-direction: row;
    align-items: center;
    padding: 8px 10px;
    margin-bottom: 2px;
    border-radius: 6px;
    transition-duration: 0.15s;
    transition-property: background-color;
    min-height: 30px;
    width: 100%;
    overflow-x: hidden;
}

.yucp-sort-option:hover {
    background-color: rgba(255, 255, 255, 0.04);
}

.yucp-sort-option.selected {
    background-color: rgba(54, 191, 177, 0.12);
}

.yucp-sort-indicator {
    width: 16px;
    height: 16px;
    min-width: 16px;
    margin-right: 10px;
    border-width: 2px;
    border-color: rgba(255, 255, 255, 0.2);
    border-radius: 50%;
    background-color: transparent;
    flex-shrink: 0;
}

.yucp-sort-option.selected .yucp-sort-indicator {
    border-color: rgba(54, 191, 177, 0.8);
    background-color: rgba(54, 191, 177, 0.3);
}

.yucp-sort-indicator-dot {
    width: 6px;
    height: 6px;
    background-color: rgba(54, 191, 177, 1);
    border-radius: 50%;
    position: absolute;
    left: 5px;
    top: 5px;
}

.yucp-sort-option-label {
    flex-grow: 1;
    font-size: 12px;
    color: rgba(255, 255, 255, 0.9);
    overflow-x: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

/* Chip/Badge - Pill-shaped tags */
.yucp-chip {
    flex-direction: row;
    align-items: center;
    padding: 4px 10px;
    background-color: rgba(54, 191, 177, 0.12);
    border-width: 1px;
    border-color: rgba(54, 191, 177, 0.25);
    border-radius: 12px;
    font-size: 11px;
    color: rgba(54, 191, 177, 0.95);
    transition-duration: 0.15s;
    transition-property: background-color, border-color;
    height: 22px;
}

.yucp-chip > * {
    margin-right: 6px;
}

.yucp-chip > *:last-child {
    margin-right: 0;
}

.yucp-chip:hover {
    background-color: rgba(54, 191, 177, 0.18);
    border-color: rgba(54, 191, 177, 0.35);
}

.yucp-chip-remove {
    width: 14px;
    height: 14px;
    min-width: 14px;
    padding: 0;
    margin: 0;
    margin-left: 4px;
    background-color: transparent;
    border-width: 0;
    color: rgba(54, 191, 177, 0.7);
    font-size: 12px;
    -unity-text-align: middle-center;
    border-radius: 3px;
}

.yucp-chip-remove:hover {
    background-color: rgba(226, 74, 74, 0.2);
    color: rgba(226, 74, 74, 1);
}

.yucp-chip-label {
    font-size: 11px;
    white-space: nowrap;
}

/* Profile list item chips - smaller variant */
.yucp-profile-tag-chip {
    padding: 2px 8px;
    font-size: 9px;
    height: 18px;
    margin-right: 4px;
    margin-bottom: 4px;
}

.yucp-clickable-tag {
    cursor: link;
}

.yucp-clickable-tag:hover {
    background-color: rgba(54, 191, 177, 0.25);
    border-color: rgba(54, 191, 177, 0.5);
}

/* ============================================================================
   TAGS EDITOR - Profile editor tags section
   ============================================================================ */
.yucp-tags-section {
    margin-top: 16px;
}

.yucp-tags-preset-container {
    flex-direction: column;
    margin-bottom: 16px;
}

.yucp-tags-preset-container > * {
    margin-bottom: 4px;
}

.yucp-tags-custom-container {
    flex-direction: row;
    flex-wrap: wrap;
    margin-bottom: 12px;
}

.yucp-tags-custom-container > * {
    margin-right: 8px;
    margin-bottom: 8px;
}

.yucp-tags-input-row {
    flex-direction: row;
    align-items: center;
    margin-top: 8px;
}

.yucp-tags-input-row > * {
    margin-right: 8px;
}

.yucp-tags-input-row > *:last-child {
    margin-right: 0;
}

.yucp-tags-input-row .yucp-input {
    flex-grow: 1;
    min-width: 200px;
}

/* Popover Scrollable Content */
.yucp-popover-scroll {
    max-height: 400px;
    overflow-x: hidden;
    overflow-y: auto;
    min-width: 0;
}

/* ============================================================================
   EXPORT INSPECTOR - PackageExporter specific
   ============================================================================ */
.yucp-inspector-header {
    flex-direction: row;
    align-items: center;
    justify-content: space-between;
    margin-bottom: 8px;
}

.yucp-inspector-header .yucp-section-title {
    margin-bottom: 0;
    flex-grow: 1;
}

.yucp-inspector-search {
    flex-grow: 1;
    flex-shrink: 1;
    min-width: 0;
    margin-right: 8px;
    overflow: hidden;
}

.yucp-inspector-filters {
    flex-direction: row;
    align-items: center;
    margin-bottom: 8px;
}

.yucp-inspector-list {
    background-color: #1a1a1a;
    border-width: 1px;
    border-color: #2a2a2a;
    border-radius: 8px;
    padding: 8px;
    min-height: 200px;
    /* max-height removed to allow resizing - controlled by code */
    overflow: hidden;
}

.yucp-inspector-list > * {
    max-width: 100%;
}

.yucp-asset-item {
    flex-direction: row;
    align-items: center;
    padding: 6px 8px;
    margin-bottom: 2px;
    margin-left: 0;
    background-color: #2a2a2a;
    border-left-width: 2px;
    border-left-color: transparent;
    border-radius: 6px;
    transition-duration: 0.2s, 0.2s, 0.2s;
    transition-property: background-color, border-left-color, scale;
    transition-timing-function: ease-out, ease-out, ease-out;
    scale: 1 1;
}

.yucp-asset-item:hover {
    background-color: #3a3a3a;
    border-left-color: rgba(54, 191, 177, 0.4);
    scale: 1.005 1.005;
}

.yucp-asset-item-checkbox {
    margin-right: 8px;
}

.yucp-asset-item-icon {
    width: 20px;
    height: 20px;
    margin-right: 8px;
    -unity-text-align: middle-center;
    font-size: 11px;
    color: #36BFB1;
}

.yucp-asset-item-name {
    flex-grow: 1;
    font-size: 11px;
    color: #ffffff;
}

.yucp-asset-item-name:hover {
    color: #36BFB1;
}

.yucp-asset-item-type {
    font-size: 10px;
    color: #808080;
    margin-right: 12px;
}

.yucp-asset-item-size {
    font-size: 10px;
    color: #808080;
}

/* ============================================================================
   EXPORT ACTIONS - PackageExporter specific
   ============================================================================ */
.yucp-export-buttons {
    flex-direction: row;
    justify-content: flex-end;
    align-items: center;
}

.yucp-export-info {
    flex-grow: 1;
    flex-direction: row;
    align-items: center;
}

.yucp-multi-select-info {
    padding: 8px 12px;
    background-color: rgba(54, 191, 177, 0.15);
    border-left-width: 0;
    border-radius: 6px;
    margin-right: 16px;
}

.yucp-multi-select-text {
    font-size: 12px;
    color: #36BFB1;
}

/* ============================================================================
   FOLDER LIST - PackageExporter specific
   ============================================================================ */
.yucp-folder-list {
    background-color: #1a1a1a;
    border-width: 1px;
    border-color: #2a2a2a;
    border-radius: 8px;
    padding: 8px;
    min-height: 100px;
}

.yucp-folder-item {
    flex-direction: row;
    align-items: center;
    padding: 6px 8px;
    background-color: #2a2a2a;
    margin-bottom: 4px;
    border-radius: 6px;
    overflow: hidden;
    transition-duration: 0.2s, 0.2s;
    transition-property: background-color, scale;
    transition-timing-function: ease-out, ease-out;
    scale: 1 1;
}

.yucp-folder-item:hover {
    background-color: #3a3a3a;
    scale: 1.005 1.005;
}

.yucp-folder-item-path {
    flex-grow: 1;
    flex-shrink: 1;
    min-width: 0;
    font-size: 11px;
    color: #ffffff;
    overflow: hidden;
    text-overflow: ellipsis;
}

.yucp-folder-item-remove {
    width: 20px;
    min-width: 20px;
    height: 20px;
    padding: 0;
    margin-left: 8px;
    background-color: #3a3a3a;
    color: #ffffff;
    font-size: 14px;
    border-radius: 4px;
    -unity-text-align: middle-center;
    flex-shrink: 0;
    transition-duration: 0.2s, 0.2s;
    transition-property: background-color, scale;
    transition-timing-function: ease-out, ease-out;
    scale: 1 1;
}

.yucp-folder-item-remove:hover {
    background-color: #E24A4A;
    scale: 1.1 1.1;
}

/* ============================================================================
   RESPONSIVE BREAKPOINTS - PackageExporter specific overrides
   ============================================================================ */
.yucp-window-narrow .yucp-export-buttons {
    flex-direction: column;
}

.yucp-window-narrow .yucp-export-buttons Button {
    width: 100%;
    margin: 4px 0;
}

.yucp-window-narrow .yucp-export-info {
    margin-bottom: 8px;
}

/* ============================================================================
   EXPORT INSPECTOR - Responsive Design
   ============================================================================ */
.yucp-window-narrow .yucp-inspector-header {
    flex-direction: column;
    align-items: flex-start;
}

.yucp-window-narrow .yucp-inspector-header .yucp-section-title {
    width: 100%;
    margin-bottom: 8px;
}

.yucp-window-narrow .yucp-inspector-header Button {
    width: 100%;
    margin-top: 0;
    margin-bottom: 8px;
    margin-left: 0;
    margin-right: 0;
}

.yucp-window-medium .yucp-inspector-header {
    flex-wrap: wrap;
}

.yucp-window-medium .yucp-inspector-header .yucp-section-title {
    flex: 1 1 auto;
    min-width: 200px;
}

/* Inspector action buttons (Scan Assets, Clear Scan) */
.yucp-inspector-action-buttons {
    flex-direction: row;
}

.yucp-window-narrow .yucp-inspector-action-buttons {
    flex-direction: column;
}

.yucp-window-narrow .yucp-inspector-action-buttons Button {
    width: 100%;
    margin: 4px 0;
    flex-grow: 1;
}

.yucp-window-medium .yucp-inspector-action-buttons Button {
    flex: 1 1 auto;
    min-width: 120px;
}

.yucp-inspector-search-row {
    flex-direction: row;
    min-width: 0;
    max-width: 100%;
    width: 100%;
}

.yucp-window-narrow .yucp-inspector-search-row {
    flex-direction: column;
    width: 100%;
    max-width: 100%;
    margin-left: 0;
    margin-right: 0;
    padding-left: 0;
    padding-right: 0;
}

.yucp-window-narrow .yucp-inspector-search-row .yucp-input {
    width: 100%;
    max-width: 100%;
    margin-right: 0;
    margin-bottom: 8px;
    margin-left: 0;
    flex-grow: 1;
    min-width: 0;
}

.yucp-window-narrow .yucp-inspector-search-row Button {
    width: 100%;
    max-width: 100%;
    margin-top: 0;
    margin-bottom: 8px;
    margin-left: 0;
    margin-right: 0;
    min-width: 0;
}

.yucp-window-medium .yucp-inspector-search-row .yucp-input {
    flex: 1 1 auto;
    min-width: 200px;
}

.yucp-inspector-filter-toggles {
    flex-direction: row;
    min-width: 0;
    max-width: 100%;
    width: 100%;
}

.yucp-window-narrow .yucp-inspector-filter-toggles {
    flex-direction: column;
    align-items: flex-start;
    width: 100%;
    max-width: 100%;
    margin-right: 0;
    margin-left: 0;
    padding-left: 0;
    padding-right: 0;
}

.yucp-window-narrow .yucp-inspector-filter-toggles .yucp-toggle {
    width: 100%;
    max-width: 100%;
    margin-bottom: 8px;
    margin-right: 0;
    margin-left: 0;
    min-width: 0;
}

.yucp-window-narrow .yucp-inspector-filter-toggles .unity-toggle {
    width: 100%;
    max-width: 100%;
}

.yucp-window-medium .yucp-inspector-filter-toggles {
    flex-wrap: wrap;
}

.yucp-window-medium .yucp-inspector-filter-toggles .yucp-toggle {
    margin-right: 16px;
    margin-bottom: 8px;
}

.yucp-window-narrow .yucp-section {
    min-width: 0;
    max-width: 100%;
    width: 100%;
}

.yucp-window-narrow .yucp-section > * {
    max-width: 100%;
    min-width: 0;
}

.yucp-inspector-list-header {
    flex-direction: row;
    justify-content: space-between;
    min-width: 0;
    max-width: 100%;
    width: 100%;
}

.yucp-window-narrow .yucp-inspector-list-header {
    flex-direction: column;
    align-items: flex-start;
}

.yucp-window-narrow .yucp-inspector-list-header Label {
    width: 100%;
    margin-bottom: 8px;
}

.yucp-inspector-list-actions {
    flex-direction: row;
    min-width: 0;
    max-width: 100%;
}

.yucp-window-narrow .yucp-inspector-list-actions {
    width: 100%;
    max-width: 100%;
    flex-direction: column;
    margin-left: 0;
    margin-right: 0;
}

.yucp-window-narrow .yucp-inspector-list-actions Button {
    width: 100%;
    max-width: 100%;
    margin: 4px 0;
    margin-left: 0;
    margin-right: 0;
}

/* Dependencies list container */
.yucp-dependency-list-container {
    width: 100%;
    max-width: 100%;
    min-width: 0;
    overflow: hidden;
}

.yucp-window-narrow .yucp-dependency-list-container {
    width: 100%;
    max-width: 100%;
}

/* Dependencies cards */
.yucp-dependency-card {
    max-width: 100%;
    min-width: 0;
    overflow: hidden;
}

.yucp-dependency-card-header {
    flex-direction: row;
    align-items: center;
    min-width: 0;
    max-width: 100%;
    overflow: hidden;
}

.yucp-dependency-card-name {
    flex-grow: 1;
    flex-shrink: 1;
    min-width: 0;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.yucp-window-narrow .yucp-dependency-card {
    width: 100%;
    max-width: 100%;
    overflow: visible;
}

.yucp-window-narrow .yucp-dependency-card-header {
    flex-direction: row;
    align-items: center;
    width: 100%;
    max-width: 100%;
    flex-wrap: wrap;
}

