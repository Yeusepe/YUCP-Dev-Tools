    right: 0;
    bottom: 0;
    -unity-background-scale-mode: scale-and-crop;
    -unity-background-image-tint-color: rgba(255, 255, 255, 1);
    background-color: transparent;
}

.yucp-banner-gradient-overlay {
    position: absolute;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: transparent;
    -unity-background-scale-mode: stretch-to-fill;
}

.yucp-banner-change-button {
    position: absolute;
    height: 24px;
    padding-left: 12px;
    padding-right: 12px;
    font-size: 11px;
    background-color: rgba(50, 50, 50, 0.95);
    border-width: 1px;
    border-color: rgba(65, 65, 65, 0.9);
    color: #e0e0e0;
    border-radius: 4px;
    justify-content: center;
    align-items: center;
}

.yucp-banner-change-button:hover {
    background-color: rgba(60, 60, 60, 0.95);
    border-color: rgba(75, 75, 75, 0.9);
}

.yucp-banner-change-button:active {
    background-color: rgba(40, 40, 40, 0.95);
}


/* Responsive banner */
.yucp-window-narrow .yucp-banner-container {
    height: 300px;
}

.yucp-window-medium .yucp-banner-container {
    height: 350px;
}

/* ============================================================================
   RESIZE HANDLE - For resizable left pane
   ============================================================================ */
.yucp-resize-handle {
    width: 4px;
    background-color: rgba(42, 42, 42, 0.3);
    flex-shrink: 0;
    transition-property: background-color;
    transition-duration: 0.2s;
    transition-timing-function: ease-out;
}

.yucp-resize-handle:hover {
    background-color: rgba(54, 191, 177, 0.6);
}

.yucp-resize-handle:active {
    background-color: rgba(54, 191, 177, 0.8);
}

/* ============================================================================
   ONBOARDING - Tour UI Styles
   ============================================================================ */

.yucp-onboarding-overlay {
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
}

.yucp-onboarding-dimmer {
    position: absolute;
    background-color: rgba(0, 0, 0, 0.7);
}

.yucp-onboarding-spotlight-border {
    position: absolute;
    border-width: 2px;
    border-color: #36BFB1; /* Accent color */
    border-radius: 8px;
    background-color: transparent;
    opacity: 0.8;
    transition-duration: 0.1s;
    transition-property: all;
    transition-timing-function: ease-out;
}

.yucp-onboarding-card {
    position: absolute;
    width: 380px;
    padding: 24px;
    background-color: #1e1e1e;
    border-width: 1px;
    border-color: rgba(255, 255, 255, 0.08);
    border-radius: 12px;
}

.yucp-onboarding-title {
    font-size: 16px;
    color: #ffffff;
    margin-bottom: 8px;
    flex-grow: 1;
}

.yucp-onboarding-counter {
    font-size: 10px;
    color: rgba(255, 255, 255, 0.4);
    align-self: flex-start;
    margin-top: 4px;
}

.yucp-onboarding-description {
    font-size: 12px;
    margin-bottom: 12px;
    white-space: normal;
    color: #d4d4d4;
    line-height: 18px; /* 1.4em approx */
}

/* Button consistency in onboarding card */
.yucp-onboarding-card Button {
    height: 32px;
    border-radius: 6px;
    font-size: 12px;
    background-color: rgba(255, 255, 255, 0.05);
    border-width: 1px;
    border-color: rgba(255, 255, 255, 0.1);
    color: #e0e0e0;
    margin-left: 0;
    margin-right: 0;
    padding-left: 16px;
    padding-right: 16px;
    transition-duration: 0.15s;
    transition-property: background-color, border-color, scale;
}

.yucp-onboarding-card Button:hover {
    background-color: rgba(255, 255, 255, 0.1);
    border-color: rgba(255, 255, 255, 0.2);
}

.yucp-onboarding-card Button:active {
    background-color: rgba(255, 255, 255, 0.02);
}

.yucp-onboarding-card .yucp-button-primary {
    background-color: rgba(54, 191, 177, 0.2);
    border-color: rgba(54, 191, 177, 0.5);
    color: #36BFB1;
}

.yucp-onboarding-card .yucp-button-primary:hover {
    background-color: rgba(54, 191, 177, 0.3);
    border-color: rgba(54, 191, 177, 0.7);
}

.yucp-onboarding-card .yucp-button-text {
    background-color: transparent;
    border-width: 0;
    color: rgba(255, 255, 255, 0.5);
}

.yucp-onboarding-card .yucp-button-text:hover {
    background-color: rgba(255, 255, 255, 0.05);
    color: rgba(255, 255, 255, 0.8);
    border-width: 0;
}

/* ============================================================================
   EXPORT ACTIONS OVERLAY - Teaching Tip Style
   ============================================================================ */
.yucp-teaching-tip-overlay {
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    justify-content: flex-end;
    align-items: flex-end;
}

.yucp-teaching-tip-container {
    background-color: #2b2b2b;
    border-radius: 8px;
    padding: 16px;
    min-width: 250px;
    border-width: 1px;
    border-color: rgba(54, 191, 177, 0.5); /* Brand color border */
    background-image: none;
}

.yucp-teaching-tip-title {
    font-size: 14px;
    -unity-font-style: bold;
    color: #ffffff;
    margin-bottom: 4px;
}

.yucp-teaching-tip-description {
    font-size: 12px;
    color: #cccccc;
    margin-bottom: 12px;
    white-space: normal;
}

.yucp-teaching-tip-divider {
    height: 1px;
    background-color: rgba(255, 255, 255, 0.1);
    margin-bottom: 12px;
}
